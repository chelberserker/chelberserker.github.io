	pages = {9306--9312},
	file = {Full Text PDF:C\:\\Users\\Егор\\Zotero\\storage\\CQ7Q3PDS\\Keith et al. - 2020 - Independently Tuning Elastomer Softness and Firmne.pdf:application/pdf},
}

