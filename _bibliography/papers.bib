	author = {Liontos, George and Manesi, Gkreti-Maria and Moutsios, Ioannis and Moschovas, Dimitrios and Piryazev, Alexey A. and Bersenev, Egor A. and Ivanov, Dimitri A. and Avgeropoulos, Apostolos},
