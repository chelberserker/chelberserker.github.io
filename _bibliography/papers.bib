	pages = {3022--3030},
	file = {Full Text PDF:C\:\\Users\\Егор\\Zotero\\storage\\QIMWAJAZ\\Dashtimoghadam et al. - 2022 - Super-soft, firm, and strong elastomers toward rep.pdf:application/pdf;Supplementary Information PDF:C\:\\Users\\Егор\\Zotero\\storage\\QTEI58LF\\Dashtimoghadam et al. - 2022 - Super-soft, firm, and strong elastomers toward rep.pdf:application/pdf},
